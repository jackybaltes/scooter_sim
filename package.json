{
  "name": "scooter-game",
  "version": "0.0.1",
  "description": "Scooter Game for FIRA 2021 SDE",
  "main": "src/URDFLoader.js",
  "type": "module",
  "scripts": {
    "start": "concurrently --kill-others \"rollup -c -w\" \"( cd bundle; static-server)\" ",
    "build": "tsc && npx rollup -c",
    "dist": "rm -r ./dist/* && cp -aR ./assets ./dist && cp -aR ./html/* ./dist/ && cp ./bundle/bundle.* ./dist",
    "test": "jest",
    "lint": "eslint \"./src/*.js\" \"./test/*.js\" && tsc -p tsconfig.json --noEmit",
    "deploy": "gh-pages -d dist"
  },
  "files": [
    "src/*.ts",
    "html/*",
    "assets/*"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackybaltes/scooter_sim.git"
  },
  "bugs": {
    "url": "https://github.com/jackybaltes/scooter_sim.git/issues"
  },
  "homepage": "https://github.com/jackybaltes/scooter_sim.git/#readme",
  "keywords": [
    "javascript",
    "threejs",
    "graphics",
    "fira",
    "robotics",
    "webcomponents"
  ],
  "peerDependencies": {
    "three": "^0.128.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/three": "^0.129.1",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "babel-jest": "^25.4.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-jest": "^24.1.0",
    "gh-pages": "^3.2.3",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "jsdom": "^16.4.0",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "rollup": "^2.29.0",
    "rollup-plugin-copy": "^3.4.0",
    "static-server": "^3.0.0",
    "three": "^0.128.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "ml-matrix": "^6.8.0",
    "typewiz": "^1.2.4"
  }
}
